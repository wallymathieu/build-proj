<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Template for creating build.proj file</Description>
    <Authors>Oskar Gewalli</Authors>
    <PackageProjectUrl>https://github.com/wallymathieu/build-proj</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/wallymathieu/build-proj/blob/master/LICENSE.md</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/wallymathieu/build-proj.git</RepositoryUrl>
    <PackageTags>template</PackageTags>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageType>Template</PackageType>
    <NoBuild>true</NoBuild>
    <IncludeBuildOutput>false</IncludeBuildOutput>
    <DisableImplicitFrameworkReferences>true</DisableImplicitFrameworkReferences>
    <TargetFramework>netstandard0.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="Content/**/*" Exclude="Content/**/packages/**/*;Content/**/.fake/**;Content/**/bin/**/**/*;Content/**/dist/*;Content/**/temp/*;Content/**/bin/**/**/*;Content/**/obj/**/**/*;Content/**/paket-files/**;" >
        <PackagePath>Content\</PackagePath>
    </Content>
  </ItemGroup>
</Project>
